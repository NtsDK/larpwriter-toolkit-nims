{
  "name": "nims-app",
  "version": "0.7.2",
  "description": "Story master toolkit NIMS",
  "main": "",
  "scripts": {
    "vtmcl-dev": "./node_modules/.bin/gulp dev --configFile \"config/vtmcl-config.json\"",
    "measurelook-dev": "./node_modules/.bin/gulp dev --configFile \"config/measurelook-config.json\"",
    "nims-wp-start": "./node_modules/.bin/webpack-dev-server --env.mode=development",
    "watch:stan000dalone": "./node_modules/.bin/webpack-dev-server --env.mode=development",
    "watch:standalone": "webpack-dev-server --mode=development",
    "nims-wp-start-server": "./node_modules/.bin/webpack-dev-server --env.mode=development  --env.product=server",
    "nims-wp-start-prod": "./node_modules/.bin/webpack-dev-server --config config\\nims.webpack.dev.js --env.mode=production",
    "nims-wp-watch-server": "webpack --watch --env.product=server",
    "watch:server": "webpack --watch --env.product=server",
    "nims-wp-build-server": "webpack --env.product=server",
    "nims-wp-build-stand": "webpack --env.product=standalone",
    "nims-wp-build-stand-stats": "webpack --env.product=standalone --env.mode=production --profile --json > stats.json",
    "nims-dev": "./node_modules/.bin/gulp dev --configFile \"config/nims-config.json\"",
    "nims-live": "node live.js --configFile \"config/nims-config.json\"",
    "nims-reload": "cd ../nims-dist && reload -b -s nims.html",
    "nims-dist": "./node_modules/.bin/gulp dist:final --configFile \"config/nims-config.json\"",
    "vtmcl-dist": "./node_modules/.bin/gulp dist:final --configFile \"config/vtmcl-config.json\"",
    "measurelook-dist": "./node_modules/.bin/gulp dist:final --configFile \"config/measurelook-config.json\"",
    "nims-server": "node --inspect ./server/bin/www --configFile \"server/config/nims-frontend-local.json\"  --globalConfigFile \"server/config/nims-frontend-global.json\"",
    "nims-nodemon": "nodemon ./server/bin/www --configFile \"server/config/nims-frontend-local.json\"  --globalConfigFile \"server/config/nims-frontend-global.json\"",
    "pppprenims-wp-watch-server": "npm run nims-lint",
    "nims-lint": "npm run core-eclint && npm run nims-eclint && npm run core-eslint && npm run nims-eslint",
    "core-eclint": "eclint fix app\\core\\js && eclint fix dbms\\core && eclint fix config && eclint fix server",
    "nims-eclint": "eclint fix app\\nims && eclint fix dbms\\nims",
    "core-eslint": "eslint app\\core\\js --fix && eslint dbms\\core --fix && eslint config --fix && eslint server --fix",
    "nims-eslint": "eslint app\\nims --fix && eslint dbms\\nims --fix",
    "nims-lint2": "\"node_modules/.bin/eslint\" app\\nims\\js --fix && \"node_modules/.bin/eslint\" app\\nims\\specs --fix"
  },
  "author": "Timofey Rechkalov (NtsDK)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/NtsDK/larpwriter-toolkit-nims"
  },
