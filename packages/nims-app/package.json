  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "ajv": "^6.12.2",
    "nims-app-core": "0.7.2",
    "bootstrap-sass": "^3.4.1",
    "bootstrap-slider": "^10.6.2",
    "chart.js": "^2.9.3",
    "d3": "^5.16.0",
    "dateformat": "^3.0.3",
    "nims-dbms-core": "0.7.2",
    "nims-dbms": "0.7.2",
    "docxtemplater": "^3.17.6",
    "file-saver": "^2.0.2",
    "jquery": "^3.5.1",
    "jquery-datetimepicker": "^2.5.21",
    "jszip": "^3.4.0",
    "markdown-it": "^11.0.0",
    "moment": "^2.26.0",
    "mustache": "^4.0.1",
    "nconf": "^0.10.0",
    "ramda": "^0.27.0",
    "nims-resources": "0.7.2",
    "select2": "^4.0.13",
    "vex-dialog": "^1.1.0",
    "vex-js": "^4.1.0",
    "vis": "^4.21.0"
  }
}
